<div id="scheduleContent">
  <div id="scheduleBar">
    <div id="switch">
      <span id="leftSwitch" class="scheduleswitch">&#9664;</span>
      <span id="scheduleNum">0</span> / {{schedules.length}}
      <span id="rightSwitch" class="scheduleswitch">&#9654;</span>
    </div>
    <div id="crnList"> </div>
  </div>
  <div id="schedulesList">
    {{#schedules}}
      <schedule-preview data-index="{{@index}}"></schedule-preview>
    {{/schedules}}
  </div>
  <div id="scheduleContainer"/>
</div>
