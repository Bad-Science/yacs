digraph models_diagram {
	graph[overlap=false, splines=true, bgcolor="none"]
	"Course" [shape=Mrecord, label="{Course|id :integer\ldepartment_id :integer\lname :character varying\lnumber :integer\lmin_credits :integer\lmax_credits :integer\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"Department" [shape=Mrecord, label="{Department|id :integer\lcode :character varying\lname :character varying\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"Period" [shape=Mrecord, label="{Period|id :integer\lsection_id :integer\ltime :character varying\lperiod_type :character varying\llocation :character varying\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"PeriodsProfessor" [shape=Mrecord, label="{PeriodsProfessor|id :integer\lprofessor_id :integer\lperiod_id :integer\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"Professor" [shape=Mrecord, label="{Professor|id :integer\lname :character varying\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"Section" [shape=Mrecord, label="{Section|id :integer\lname :character varying\lcrn :integer\lcourse_id :integer\lseats :integer\lseats_taken :integer\lcreated_at :timestamp without time zone\lupdated_at :timestamp without time zone\l}"]
	"Course" -> "Section" [arrowtail=odot, arrowhead=crow, dir=both color="#2F0DD5"]
	"Department" -> "Course" [arrowtail=odot, arrowhead=crow, dir=both color="#E4DC0C"]
	"Period" -> "Professor" [arrowtail=crow, arrowhead=crow, dir=both color="#322389"]
	"Section" -> "Period" [arrowtail=odot, arrowhead=crow, dir=both color="#25BFD7"]
	"Section" -> "Professor" [arrowtail=crow, arrowhead=crow, dir=both color="#A4ADA2"]
}
